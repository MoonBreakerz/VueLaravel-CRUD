{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { TarefasService } from \"../../services/TarefasService\";\nexport default {\n  name: \"TarefasComponent\",\n  data() {\n    return {\n      tarefaId: this.$route.params.tarefaId,\n      tarefas: []\n    };\n  },\n  async created() {\n    let response = await TarefasService.getAllTarefas();\n    this.tarefas = response.data;\n  },\n  methods: {\n    async clickDeleteTarefa(tarefasid) {\n      let response = await TarefasService.deletarTarefa(tarefasid);\n      if (response.status === 200) {\n        let response = await TarefasService.getAllTarefas();\n        this.tarefas = response.data;\n      }\n    },\n    async finalizarTarefa(tarefas, tarefasid) {\n      this.tarefas.finalizado = \"sim\";\n      let response = await TarefasService.finalizaTarefa(this.tarefa, this.tarefaId);\n      if (response) {\n        return this.$router.push('/');\n      } else {\n        return this.$router.push(`/tarefas/${this.tarefaId}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["TarefasService","name","data","tarefaId","$route","params","tarefas","created","response","getAllTarefas","methods","clickDeleteTarefa","tarefasid","deletarTarefa","status","finalizarTarefa","finalizado","finalizaTarefa","tarefa","$router","push"],"sources":["C:\\Users\\gabri\\Documents\\inbraep\\frontend\\src\\views\\TarefasComponent.vue"],"sourcesContent":["<template>\r\n    <div class=\"container mt-3\">\r\n        <div class=\"row\">\r\n            <div class=\"col\">\r\n                <p class=\"h3 text-sucess fw-bold\">\r\n                    Adicionar Tarefa\r\n                    <router-link to=\"/tarefas/adicionar\"\r\n                                 class=\"btn btn-success btn-sm\"><i class=\"fa fa-plus-circle\"></i> Nova</router-link>\r\n                </p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"container mt-3\" v-if=\"tarefas.length > 0\">\r\n        <div class=\"row\">\r\n            <div class=\"col\">\r\n                <div class=\"card\" v-for=\"tarefas of tarefas\" :key=\"tarefas\">\r\n                    <div class=\"card-body\">\r\n                        <div class=\"row align-items-center\">\r\n                            <div class=\"col-sm-10\">\r\n                                <div class=\"card-body\">\r\n                                    {{tarefas.descricao}}\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"col-sm-1 d-flex-column justify-content-center align-items-center\">\r\n                                <router-link v-if=\"tarefas.finalizado === 'nao'\" :to=\"`/tarefas/editar/${tarefas.id}`\" class=\"btn btn-primary\">\r\n                                    <i class=\"fa fa-pen\"></i>\r\n                                </router-link>\r\n                                <button class=\"btn btn-secondary my-1\"\r\n                                        @click=\"finalizarTarefa(tarefas.id)\">\r\n                                    <i class=\"fa fa-check\" style=\"color: #4cff00\"></i> \r\n                                </button>\r\n                                <button class=\"btn btn-danger my-1\"\r\n                                        @click=\"clickDeleteTarefa(tarefas.id)\">\r\n                                    <i class=\"fa fa-trash\"></i>\r\n                                </button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { TarefasService } from \"../../services/TarefasService\";\r\n\r\n    export default {\r\n        name: \"TarefasComponent\",\r\n        data() {\r\n            return {\r\n                tarefaId: this.$route.params.tarefaId,\r\n                tarefas: [\r\n                ],\r\n            };\r\n        },\r\n        async created() {\r\n            let response = await TarefasService.getAllTarefas();\r\n            this.tarefas = response.data;\r\n        },\r\n        methods: {\r\n            async clickDeleteTarefa(tarefasid) {\r\n                let response = await TarefasService.deletarTarefa(tarefasid);\r\n                if (response.status === 200) {\r\n                    let response = await TarefasService.getAllTarefas();\r\n                    this.tarefas = response.data;\r\n                }\r\n            },\r\n            async finalizarTarefa(tarefas, tarefasid) {\r\n                this.tarefas.finalizado =\"sim\"\r\n                let response = await TarefasService.finalizaTarefa(this.tarefa, this.tarefaId);\r\n                if (response) {\r\n                    return this.$router.push('/');\r\n                } else {\r\n                    return this.$router.push(`/tarefas/${this.tarefaId}`);\r\n                }\r\n            }\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"],"mappings":";AA6CI,SAASA,cAAa,QAAS,+BAA+B;AAE9D,eAAe;EACXC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,QAAQ;MACrCG,OAAO,EAAE;IAEb,CAAC;EACL,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,IAAIC,QAAO,GAAI,MAAMR,cAAc,CAACS,aAAa,CAAC,CAAC;IACnD,IAAI,CAACH,OAAM,GAAIE,QAAQ,CAACN,IAAI;EAChC,CAAC;EACDQ,OAAO,EAAE;IACL,MAAMC,iBAAiBA,CAACC,SAAS,EAAE;MAC/B,IAAIJ,QAAO,GAAI,MAAMR,cAAc,CAACa,aAAa,CAACD,SAAS,CAAC;MAC5D,IAAIJ,QAAQ,CAACM,MAAK,KAAM,GAAG,EAAE;QACzB,IAAIN,QAAO,GAAI,MAAMR,cAAc,CAACS,aAAa,CAAC,CAAC;QACnD,IAAI,CAACH,OAAM,GAAIE,QAAQ,CAACN,IAAI;MAChC;IACJ,CAAC;IACD,MAAMa,eAAeA,CAACT,OAAO,EAAEM,SAAS,EAAE;MACtC,IAAI,CAACN,OAAO,CAACU,UAAS,GAAG,KAAI;MAC7B,IAAIR,QAAO,GAAI,MAAMR,cAAc,CAACiB,cAAc,CAAC,IAAI,CAACC,MAAM,EAAE,IAAI,CAACf,QAAQ,CAAC;MAC9E,IAAIK,QAAQ,EAAE;QACV,OAAO,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACjC,OAAO;QACH,OAAO,IAAI,CAACD,OAAO,CAACC,IAAI,CAAE,YAAW,IAAI,CAACjB,QAAS,EAAC,CAAC;MACzD;IACJ;EACJ;AACJ,CAAC"