{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { TarefasService } from \"../../services/TarefasService\";\nexport default {\n  name: \"TarefasComponent\",\n  data() {\n    return {\n      tarefas: []\n    };\n  },\n  async created() {\n    let response = await TarefasService.getAllTarefas();\n    this.tarefas = response.data;\n  },\n  methods: {\n    async clickDeleteTarefa(id) {\n      let response = await TarefasService.deleteTarefa(id);\n      if (response.status === 204) {\n        let response = await TarefasService.getAllTarefas();\n        this.tarefas = response.data;\n      }\n    },\n    async finalizarTarefa(id) {\n      let tarefa = this.tarefas.find(t => t.id === id);\n      tarefa.finalizado = \"sim\";\n      let response = await TarefasService.finalizarTarefa(id, tarefa);\n      if (response) {\n        let response = await TarefasService.getAllTarefas();\n        this.tarefas = response.data;\n        return this.$router.push(\"/\");\n      } else {\n        return this.$router.push(`/tarefas/${id}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["TarefasService","name","data","tarefas","created","response","getAllTarefas","methods","clickDeleteTarefa","id","deleteTarefa","status","finalizarTarefa","tarefa","find","t","finalizado","$router","push"],"sources":["C:\\Users\\gabri\\Documents\\inbraep\\frontend\\src\\views\\TarefasComponent.vue"],"sourcesContent":["<template>\r\n    <div class=\"container mt-3\">\r\n        <div class=\"row\">\r\n            <div class=\"col\">\r\n                <p class=\"h3 text-sucess fw-bold\">\r\n                    Adicionar Tarefa\r\n                    <router-link to=\"/tarefas/adicionar\"\r\n                                 class=\"btn btn-success btn-sm\"><i class=\"fa fa-plus-circle\"></i> Nova</router-link>\r\n                </p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"container mt-3\" v-if=\"tarefas.length > 0\">\r\n        <div class=\"row\">\r\n            <div class=\"col\">\r\n                <!---- <div class=\"card\" v-for=\"tarefas of tarefas\" :key=\"tarefas\" > -->\r\n                <div class=\"card mb-3\" v-for=\"tarefa of tarefas\" :key=\"tarefa.id\"\r\n                     :class=\"{ 'text-muted': tarefa.Finalizado === 'sim', 'bg-success': tarefa.Finalizado === 'sim' }\">\r\n\r\n                    <div class=\"card-body d-flex justify-content-between align-items-center\">\r\n                        <div>\r\n                            <p class=\"card-text\">{{tarefa.Descricao}}</p>\r\n                        </div>\r\n\r\n                        <div class=\"d-flex flex-row-reverse\">\r\n                            <router-link v-if=\"tarefa.Finalizado === 'nao'\" :to=\"`/tarefas/editar/${tarefa.id}`\" class=\"btn btn-primary mr-2\">\r\n                                <i class=\"fa fa-pen\"></i>\r\n                            </router-link>\r\n\r\n                            <button class=\"btn btn-danger\" @click=\"clickDeleteTarefa(tarefa.id)\">\r\n                                <i class=\"fa fa-trash\"></i>\r\n                            </button>\r\n\r\n                            <button v-if=\"tarefa.Finalizado === 'nao'\" class=\"btn btn-secondary ml-2\" @click=\"finalizarTarefa(tarefa.id)\">\r\n                                <i class=\"fa fa-check\"></i>\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n\r\n\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import { TarefasService } from \"../../services/TarefasService\";\r\n\r\n    export default {\r\n        name: \"TarefasComponent\",\r\n        data() {\r\n            return {\r\n                tarefas: [],\r\n            };\r\n        },\r\n        async created() {\r\n            let response = await TarefasService.getAllTarefas();\r\n            this.tarefas = response.data;\r\n        },\r\n        methods: {\r\n            async clickDeleteTarefa(id) {\r\n                let response = await TarefasService.deleteTarefa(id);\r\n                if (response.status === 204) {\r\n                    let response = await TarefasService.getAllTarefas();\r\n                    this.tarefas = response.data;\r\n                }\r\n            },\r\n            async finalizarTarefa(id) {\r\n                let tarefa = this.tarefas.find((t) => t.id === id);\r\n                tarefa.finalizado = \"sim\";\r\n\r\n                let response = await TarefasService.finalizarTarefa(id, tarefa);\r\n\r\n                if (response) {\r\n                    let response = await TarefasService.getAllTarefas();\r\n                    this.tarefas = response.data;\r\n                    return this.$router.push(\"/\");\r\n                } else {\r\n                    return this.$router.push(`/tarefas/${id}`);\r\n                }\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .card-text {\r\n        color: #000000;\r\n        font-weight: bold;\r\n    }\r\n</style>\r\n"],"mappings":";AAgDI,SAASA,cAAa,QAAS,+BAA+B;AAE9D,eAAe;EACXC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,IAAIC,QAAO,GAAI,MAAML,cAAc,CAACM,aAAa,CAAC,CAAC;IACnD,IAAI,CAACH,OAAM,GAAIE,QAAQ,CAACH,IAAI;EAChC,CAAC;EACDK,OAAO,EAAE;IACL,MAAMC,iBAAiBA,CAACC,EAAE,EAAE;MACxB,IAAIJ,QAAO,GAAI,MAAML,cAAc,CAACU,YAAY,CAACD,EAAE,CAAC;MACpD,IAAIJ,QAAQ,CAACM,MAAK,KAAM,GAAG,EAAE;QACzB,IAAIN,QAAO,GAAI,MAAML,cAAc,CAACM,aAAa,CAAC,CAAC;QACnD,IAAI,CAACH,OAAM,GAAIE,QAAQ,CAACH,IAAI;MAChC;IACJ,CAAC;IACD,MAAMU,eAAeA,CAACH,EAAE,EAAE;MACtB,IAAII,MAAK,GAAI,IAAI,CAACV,OAAO,CAACW,IAAI,CAAEC,CAAC,IAAKA,CAAC,CAACN,EAAC,KAAMA,EAAE,CAAC;MAClDI,MAAM,CAACG,UAAS,GAAI,KAAK;MAEzB,IAAIX,QAAO,GAAI,MAAML,cAAc,CAACY,eAAe,CAACH,EAAE,EAAEI,MAAM,CAAC;MAE/D,IAAIR,QAAQ,EAAE;QACV,IAAIA,QAAO,GAAI,MAAML,cAAc,CAACM,aAAa,CAAC,CAAC;QACnD,IAAI,CAA